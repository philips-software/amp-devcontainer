{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "changelog-sections": [
    {
      "type": "feat",
      "section": "Features",
      "hidden": false
    },
    {
      "type": "fix",
      "section": "Bug Fixes",
      "hidden": false
    },
    {
      "type": "deps",
      "section": "Dependencies",
      "hidden": false
    }
  ],
  "include-component-in-tag": false,
  "packages": {
    ".": {
      "package-name": "amp-devcontainer",
      "release-type": "simple"
    }
  },
  "plugins": [
    "sentence-case"
  ]
}
